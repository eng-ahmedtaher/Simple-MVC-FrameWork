<IfModule mod_rewrite.c>    
    Options +FollowSymLinks
    RewriteEngine On

    RewriteCond %{REQUEST_URI} !-f
    RewriteCond %{REQUEST_URI} !-d
    RewriteCond %{REQUEST_URI} !-l

	RewriteCond %{REQUEST_FILENAME} !\.(css|js|png|jpg)$
    RewriteRule ^(.*)$ index.php?$1 [QSA,L]
</IfModule>